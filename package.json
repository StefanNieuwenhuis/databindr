{
  "name": "@stefannhs/databindr",
  "version": "0.0.1",
  "description": "A Custom Element extension for simple data binding",
  "main": "dist/index.js",
  "module": "dist/index.es.js",
  "types": "dist/types/data-bindr.component.d.ts",
  "unpkg": "dist/index.js",
  "files": [
    "dist/"
  ],
  "access": "public",
  "scripts": {
    "start": "http-server ./docs",
    "build": "node ./bin/build.js && rollup -c && cp CHANGELOG.md ./dist",
    "watch": "rollup -cw",
    "lint": "eslint --ext .js,.ts,.tsx src/",
    "commit": "git-cz",
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/StefanNieuwenhuis/databindr.git"
  },
  "keywords": [
    "databindr",
    "javascript",
    "typescript",
    "webcomponents",
    "custom-elements",
    "shadowdom",
    "databinding"
  ],
  "author": "Stefan Nieuwenhuis",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/StefanNieuwenhuis/databindr/issues"
  },
  "homepage": "https://github.com/StefanNieuwenhuis/databindr#readme",
  "dependencies": {},
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^2.10.0",
    "@typescript-eslint/parser": "^2.10.0",
    "commitizen": "^4.0.3",
    "conventional-changelog-cli": "^2.0.28",
    "cz-conventional-changelog": "^3.0.2",
    "eslint": "^6.7.2",
    "eslint-config-airbnb-base": "^14.0.0",
    "eslint-config-prettier": "^6.7.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-prettier": "^3.1.1",
    "prettier": "^1.19.1",
    "rollup": "^1.20.2",
    "rollup-plugin-terser": "^5.1.1",
    "rollup-plugin-typescript2": "^0.23.0",
    "typescript": "^3.5.3"
  },
  "peerDependencies": {},
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}